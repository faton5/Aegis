# Configuration Aegis Bot - Exemple
# Copiez ce fichier vers .env et remplissez les valeurs

# Token du bot Discord (obligatoire)
DISCORD_TOKEN=your_discord_bot_token_here

# Configuration Supabase (obligatoire si SUPABASE_ENABLED=true dans config.py)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Mode test et diagnostic (optionnel)
# Activez cette variable pour accéder aux commandes de diagnostic /test_diagnostics
# Valeurs acceptées: true, 1, yes, oui (insensible à la casse)
AEGIS_TEST_MODE=true

# Niveau de logging (optionnel, par défaut: INFO)
# Valeurs: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Salt secret pour le hachage anonyme des reporters (OBLIGATOIRE pour la sécurité)
# Générer avec: python -c "import secrets; print(secrets.token_hex(32))"
# ATTENTION: Ne jamais partager cette clé - elle protège l'anonymat des reporters
REPORTER_SALT_SECRET=your_secret_salt_here_generate_with_secrets_module